{
  "name": "flight-spotter-api",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "build": "tsc --noEmit",
    "start": "cross-env NODE_ENV=development npm-run-all build server:dev",
    "start:prod": "cross-env NODE_ENV=production ts-node src/index.ts",
    "server:dev": "nodemon -e ts,graphql -x ts-node src/index.ts"
  },
  "dependencies": {
    "apollo-datasource-rest": "^0.3.2",
    "apollo-server": "^2.4.8",
    "dotenv": "^8.0.0",
    "graphql": "^14.2.1",
    "graphql-import": "^0.7.1",
    "graphql-iso-date": "^3.6.1",
    "graphql-tools": "^4.0.4"
  },
  "devDependencies": {
    "@types/graphql": "^14.2.0",
    "cross-env": "^5.2.0",
    "nodemon": "^1.19.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.17.0",
    "ts-node": "^8.1.0",
    "tslint": "^5.16.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.4.5"
  }
}
